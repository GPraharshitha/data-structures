#include<stdio.h>
void main()
{
    int i,j,count=0,n,a[100];
    scanf("%d",&n);
    for(i=0;i<=n-1;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n-1;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(a[i]==a[j])
            count=count+1;
        }
    }
    printf("%d",count);    
}
